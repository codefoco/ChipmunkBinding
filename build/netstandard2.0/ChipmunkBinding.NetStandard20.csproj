<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>ChipmunkBinding</RootNamespace>
    <AssemblyName>ChipmunkBinding</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <SignAssembly>true</SignAssembly>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>..\..\lib\Release\netstandard2.0\ChipmunkBinding.xml</DocumentationFile>
    <OutputPath>..\..\lib\Release\netstandard2.0\</OutputPath>
    <DefineConstants>TRACE;NETSTANDARD</DefineConstants>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>..\..\lib\Debug\netstandard2.0\</OutputPath>
    <DocumentationFile>..\..\lib\Debug\netstandard2.0\ChipmunkBinding.xml</DocumentationFile>
  </PropertyGroup>
  <Import Project="..\..\src\ChipmunkBinding.Shared.projitems" Label="Shared" Condition="Exists('..\..\src\ChipmunkBinding.Shared.projitems')" />
  <Import Project="..\targets\ChipmunkBinding.Sign.targets" />
</Project>
